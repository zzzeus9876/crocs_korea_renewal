{
    "gitlens.outputLevel": "debug",
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.advanced.repositorySearchDepth": 2,
    "gitlens.defaultDateLocale": "ko-KR",
    "gitlens.advanced.caching.enabled": true,
    "gitlens.advanced.useSymmetricDifferenceNotation": false,
    "gitlens.advanced.maxSearchLines": 10000,
    "gitlens.advanced.similarityThreshold": 50,
    "gitlens.blame.ignoreWhitespace": true,
    "gitlens.views.repositories.compact": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "gitlens.views.branches.reveal": true,
    "gitlens.views.commits.reveal": true,
    "gitlens.views.repositories.autoRefresh": true,
    "gitlens.advanced.externalDiffTool": "",
    "gitlens.advanced.messages.suppressCommitHasNoPreviousCommitWarning": false,
    "gitlens.advanced.messages.suppressCommitNotFoundWarning": false,
    "gitlens.advanced.messages.suppressFileNotUnderSourceControlWarning": false,
    "gitlens.advanced.messages.suppressGitDisabledWarning": false,
    "gitlens.advanced.messages.suppressGitVersionWarning": false,
    "gitlens.advanced.messages.suppressLineUncommittedWarning": false,
    "gitlens.advanced.messages.suppressNoRepositoryWarning": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.largeFileThreshold": 2048,
    "git.useForcePushWithLease": true,
    "git.mergeEditor": true,
    "git.openDiffOnClick": true,
    "git.decorations.enabled": true,
    "git.allowForcePush": false,
    "diffEditor.renderSideBySide": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "files.encoding": "utf8",
    "files.eol": "\r\n",
    "files.associations": {
        "*.png": "image",
        "*.jpg": "image",
        "*.jpeg": "image",
        "*.gif": "image",
        "*.mp4": "video"
    },
    "editor.largeFileOptimizations": false
}